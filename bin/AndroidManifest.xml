<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.initvent.tenantsystem"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="21" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
   	<uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" /> 
   	<uses-permission android:name="android.permission.INTERNET" >
    </uses-permission>
	<uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" > 
	</uses-permission>
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" >
    </uses-permission>
	
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.initvent.tenantsystem.SplashActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
         <activity
            android:name="com.initvent.tenantsystem.MainActivity"
            android:label="@string/main_name" >
        </activity>
        <activity android:name="com.initvent.tenantsystem.LoginActivity"
                  android:label="@string/login_name" >
        </activity>
        <activity android:name="com.initvent.tenantsystem.SessionManager"></activity>
        <activity android:name="com.initvent.tenantsystem.AlertDialogManager"></activity>
        <activity android:name="com.initvent.tenantsystem.MasterActivity"></activity>
        <activity android:name="com.initvent.tenantsystem.NewBuildingActivity"></activity>
        <activity android:name="com.initvent.tenantsystem.NewTasktypeActivity"></activity>
        <activity android:name="com.initvent.tenantsystem.NewTenant"></activity>
        <activity android:name="com.initvent.tenantsystem.TaskActivity"
                  android:label="@string/task_name" >
        </activity>
        <activity android:name="com.initvent.tenantsystem.TaskUpdateActivity"
                  android:label="@string/task_name" >
        </activity>
    </application>

</manifest>
